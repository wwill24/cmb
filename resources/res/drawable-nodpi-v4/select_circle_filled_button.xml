<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="true" android:id="@+id/selected" android:drawable="@drawable/check_circle_selected_filled"/>
    <item android:state_enabled="false" android:state_checked="false" android:id="@+id/disabled" android:drawable="@drawable/check_circle_disabled"/>
    <item android:state_enabled="true" android:state_checked="false" android:id="@+id/unselected" android:drawable="@drawable/check_circle_unselected_filled"/>
</animated-selector>
