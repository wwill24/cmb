                <com.coffeemeetsbagel.cmb_views.CmbTextView android:gravity="center_vertical" android:id="@+id/safety_text" android:layout_width="match_parent" android:layout_height="60dp" android:text="@string/safety_privacy" style="@style/regular_dark_400"/>
