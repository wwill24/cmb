            <com.coffeemeetsbagel.cmb_views.CmbButton android:id="@+id/button_boost_me" android:paddingLeft="32dp" android:paddingRight="32dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="16dp" android:text="@string/boost_me" android:paddingHorizontal="32dp" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toBottomOf="@+id/boost_subtitle" style="@style/main_button"/>
