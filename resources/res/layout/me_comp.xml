                <com.coffeemeetsbagel.cmb_views.CmbTextView android:gravity="center_vertical" android:id="@+id/settings_text" android:layout_width="match_parent" android:layout_height="60dp" android:text="@string/settings" style="@style/regular_dark_400"/>
            </LinearLayout>
            <View android:background="@color/divider_color" android:layout_width="match_parent" android:layout_height="@dimen/one_dp" android:layout_marginBottom="16dp"/>
        </LinearLayout>
    </LinearLayout>
</androidx.core.widget.NestedScrollView>
