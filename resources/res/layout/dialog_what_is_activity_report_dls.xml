                <com.coffeemeetsbagel.cmb_views.CmbTextView android:id="@+id/pie_view_description" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/number_of_times_they_chat_over_numbers_of_connections" app:layout_constraintBottom_toTopOf="@+id/message_view_title" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@+id/pie_view_title" app:layout_constraintTop_toBottomOf="@+id/pie_view_title" style="@style/regular_gray_200"/>
                <ImageView android:id="@+id/icon_send_first_message" android:layout_width="48dp" android:layout_height="48dp" android:src="@drawable/ic_sends_first_message" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="@+id/message_view_title"/>
                <com.coffeemeetsbagel.cmb_views.CmbTextView android:id="@+id/message_view_title" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:text="@string/activity_report_message_header" android:layout_marginStart="@dimen/spacer_1" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toEndOf="@+id/icon_send_first_message" app:layout_constraintTop_toBottomOf="@+id/pie_view_description" style="@style/medium_dark_300"/>
                <com.coffeemeetsbagel.cmb_views.CmbTextView android:id="@+id/message_view_description" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/number_of_times_they_initiate_chat_over_numbers_of_connections" app:layout_constraintBottom_toTopOf="@+id/on_off_view_title" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@+id/message_view_title" app:layout_constraintTop_toBottomOf="@+id/message_view_title" style="@style/regular_gray_200"/>
                <ImageView android:id="@+id/icon_activeness" android:layout_width="48dp" android:layout_height="48dp" android:src="@drawable/ic_activeness" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="@+id/on_off_view_title"/>
                <com.coffeemeetsbagel.cmb_views.CmbTextView android:id="@+id/on_off_view_title" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:text="@string/active_within_72_hours" android:layout_marginStart="@dimen/spacer_1" app:layout_constraintBottom_toTopOf="@+id/on_off_view_description" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toEndOf="@+id/icon_activeness" app:layout_constraintTop_toBottomOf="@+id/message_view_description" style="@style/medium_dark_300"/>
                <com.coffeemeetsbagel.cmb_views.CmbTextView android:id="@+id/on_off_view_description" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/how_recently_they_logged_in" app:layout_constraintBottom_toTopOf="@+id/dotted_circle_view_title" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="@+id/on_off_view_title" app:layout_constraintTop_toBottomOf="@+id/on_off_view_title" style="@style/regular_gray_200"/>
                <ImageView android:id="@+id/icon_replies_within" android:layout_width="48dp" android:layout_height="48dp" android:src="@drawable/ic_replies_within" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="@+id/dotted_circle_view_title"/>
