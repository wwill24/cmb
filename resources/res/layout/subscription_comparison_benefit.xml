<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/bundle_benefit" android:layout_width="wrap_content" android:layout_height="wrap_content">
    <TextView android:gravity="center_vertical" android:id="@+id/benefit_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="3.5dp" android:minHeight="58dp" android:layout_centerVertical="true" android:layout_toStartOf="@+id/first_bundle_check" android:layout_alignParentStart="true" style="@style/regular_gray_400"/>
