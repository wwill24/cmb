        <com.coffeemeetsbagel.cmb_views.CmbTextView android:gravity="center" android:id="@+id/subscription_duration_save" android:layout_width="match_parent" android:layout_height="wrap_content" style="@style/medium_accent_300"/>
        <Space android:id="@+id/subscription_duration_bottom_divider" android:background="@color/divider_color" android:layout_width="match_parent" android:layout_height="@dimen/one_dp"/>
    </LinearLayout>
</merge>
